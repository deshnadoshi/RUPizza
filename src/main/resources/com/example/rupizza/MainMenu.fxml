<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="493.0" prefWidth="562.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rupizza.MainMenuController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="475.0" prefWidth="522.0">
         <children>
            <Button contentDisplay="BOTTOM" layoutX="267.0" layoutY="48.0" mnemonicParsing="false" onAction="#goToSpecialtyPizzas" prefHeight="127.0" prefWidth="216.0" text="Specialty Pizzas">
               <graphic>
                  <ImageView fx:id="specialty" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Button>
            <Button contentDisplay="BOTTOM" layoutX="267.0" layoutY="242.0" mnemonicParsing="false" onAction="#goToStoreOrders" prefHeight="127.0" prefWidth="216.0" text="Store Orders">
               <graphic>
                  <ImageView fx:id="storeOrders" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Button>
            <Button contentDisplay="BOTTOM" layoutX="33.0" layoutY="242.0" mnemonicParsing="false" onAction="#goToCurrentOrder" prefHeight="127.0" prefWidth="216.0" text="Current Order">
               <graphic>
                  <ImageView fx:id="currentOrder" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Button>
            <Button contentDisplay="BOTTOM" layoutX="33.0" layoutY="48.0" mnemonicParsing="false" onAction="#goToBuildYourOwn" prefHeight="127.0" prefWidth="216.0" text="Build Your Own">
               <graphic>
                  <ImageView fx:id="build" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Button>
            <Label layoutX="135.0" prefHeight="17.0" prefWidth="265.0" text="RU Pizza Main Menu">
               <font>
                  <Font name="System Bold" size="27.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</VBox>
